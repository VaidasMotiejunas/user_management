{% extends 'base.html.twig' %}

{% block title %}Admin panel{% endblock %}

{% block body %}

<h4>Group: {{group.name}} data</h4>
<table>
    <thead>
        <th>name</th>
        <th>users</th>
    </thead>
    <tbody>
        <td>{{group.name}}</td>
        <td>
        {% for user in group.users %}
            {{user.name}}
        {% endfor %}
        </td>
    </tbody>
</table>

<button type="button"><a href="{{path('index')}}">Go back to index</a></button>

{% endblock %}
